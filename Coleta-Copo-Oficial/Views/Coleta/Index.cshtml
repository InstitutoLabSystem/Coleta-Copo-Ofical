                    <div class="mr-2 w-50">
                    <p class="rotulo-instrumento">Certificado</p>
                    <input type="text" name="certificado" id="certificado" class="form-control">
                    </div>
                    <div class="mr-2 w-50 d-flex align-items-end">
                    <div>
                    <p class="rotulo-instrumento">Validade</p>
                    <input type="text" name="validade" id="validade" class="form-control">
                    </div>
                    <button style="margin:0px;" class="btn btn-primary h4 ml-2 adicionar_instrumentos">+</button>
                    </div>*@
                </div>

                <table class="table table-bordered border-secondary table-instrumentos">
                    <Br />
                    <thead class="tablestyle">
                        <tr>
                            <td>Código/Lote</td>
                            <td>Descrição</td>
                            <td>Certificado</td>
                            <td>Validade</td>

                        </tr>
                    </thead>
                    <tbody id="tbody-instrumentos">
                    @foreach (var instrumentos in Model.oInstrumentos)
                    {
                        @if (instrumentos.ativo == 1)
                        {
                                <tr>
                                    <td>@instrumentos.codigo</td>
                                    <td>@instrumentos.descricao</td>
                                    <td>@instrumentos.certificado</td>
                                    <td>@instrumentos.validade</td>
                                </tr>
                        }
                    }
                    </tbody>
                </table>

                <div class="teste">
                    <button type="submit" value="SalvarInstrumentos" class="btn btn-outline-danger"> Salvar </button>
                </div>
        </form>

        <Br />
        @if (Model.oInstrumentos.Count > 0)
        {
            <form asp-controller="Coleta" asp-action="ExcluirInstrumentos" asp-route-os="@ViewData["os"]" asp-route-orcamento="@ViewData["orcamento"]" asp-route-rev="@ViewData["Rev"]">
                <div>
                    <button type="submit" value="ExcluirInstrumentos" class="btn btn-danger"> Excluir Codigo </button>
                </div>
            </form>
        }

    </div>

    <hr style="color:darkred" />

