                            <th scope="row"> Lote </th>
                            <th scope="row"> Validade </th>
                        </tr>
                    </thead>
                    <tbody>

                        <tr>
                            <td colspan="3"> <input type="text" value="---" name="info" class="form-control" style="border: none" /> </td>
                            <td><input type="text" name="lote" value="---" class="form-control" style="border: none" /></td>
                            <td><input type="text" name="validade" value="---" class="form-control" style="border: none" /></td>
                        </tr>
                    </tbody>
                </table>

                <p><strong> Observações Gerais: </strong></p>

                <textarea name="observacoes" class="form-control" rows="3" style="border:solid; border-color:black; border-width:1px;"></textarea>
                <br />
                <div class="d-grid gap-2">
                    <button type="submit" value="SalvarMarcacao" class="btn btn-outline-danger"> Salvar </button>
                </div>
            }
            else
            {

                <table class="table table-bordered border-secondary  table table-sm">
                    <thead class="tablestyle ">
                        <tr>
                            <th colspan="5"> Materiais Utilizados </th>

                        </tr>
                        <tr>
                            <th colspan="3"> Matéria prima  </th>
                            <th scope="row"> Lote </th>
                            <th scope="row"> Validade </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td colspan="3"> <input type="text" name="info" class="form-control" style="border: none" / value="@Model.oMateriais.info"></td>
                            <td><input type="text" name="lote" class="form-control" style="border: none" / value="@Model.oMateriais.lote"></td>
                            <td><input type="text" name="validade" class="form-control" style="border: none" value="@Model.oMateriais.validade" /></td>
                        </tr>
                    </tbody>
                </table>
                <p><strong> Observações Gerais: </strong></p>

                <textarea name="observacoes" class="form-control" rows="3" style="border:solid; border-color:black; border-width:1px;" value="@Model.oMateriais.observacoes">@Model.oMateriais.observacoes</textarea>
                <br />
                <div class="d-grid gap-2">
                    <button type="submit" value="SalvarMarcacao" class="btn btn-outline-success"> Editar </button>
                </div>

            }

        </form>

        <br />

        <!--Inico de instrumentos utilizados -->
        <hr style="color:darkred" />
        <p style="text-align : center"> <strong> Instrumentos Utilizados. </strong></p>
        <hr style="color:darkred" />
        <form method="post" asp-action="PesquisarInstrumentos" asp-controller="Coleta" asp-route-os="@ViewData["os"]" asp-route-orcamento="@ViewData["orcamento"]" asp-route-rev="@ViewData["Rev"]>
            <div class="container p-3">
                <div class="d-flex align-items-end justify-content-between">
                    <div class="mr-3 w-100">
                        <p class="rotulo-instrumento">Tipo</p>
                        <input type="text" disabled class="form-control" placeholder="IM/DE/EE">
                    </div>
                    <div class="mr-3 w-100">
                        <p class="rotulo-instrumento">Código/Lote</p>
                        <input type="text" name="Codigo" class="form-control" id="codigo">
                    </div>
                    <div>
                        <p type="hidden" class="rotulo-instrumento"></p>
                        <input type="text" hidden name="instrumetosExcluir" class="form-control" id="codigo">
                    </div>
